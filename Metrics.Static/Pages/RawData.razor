@page "/RawData"
@using Metrics.Core
@using Metrics.Core.Enum
@using Metrics.Static.Components
@inherits RawDataBase

The Raw Data

@if (chartViews != null)
{
    <table>
        <tr>
            <td>Date</td>
            <td>Value</td>
        </tr>
        @foreach(var item in chartViews)
        {
            @foreach(var subitem in item)
            {
                <tr>
                    <td>@subitem.Date</td>
                    <td>@subitem.Total</td>
                </tr>
            }
        }
    </table>
}
